{
  "name": "reactive-charts",
  "version": "0.0.0",
  "description": "reusable reactive charts based on d3",
  "main": "lib/reactive-charts.js",
  "scripts": {
    "build": "npm run flow && webpack --colors --env build",
    "dev": "npm run flow && webpack-dev-server --env dev --config webpack.config.js --progress --colors --profile --watch --content-base examples/",
    "flow": "flow check",
    "lint": "eslint src/**/*.js",
    "test": "mocha --compilers js:babel-core/register --colors -w ./src/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/memee/reactive-charts.git"
  },
  "keywords": [
    "reactive",
    "frp",
    "functional",
    "charts"
  ],
  "author": "Maciej Maciaszek <maciej.maciaszek@gmail.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/memee/reactive-charts/issues"
  },
  "homepage": "https://github.com/memee/reactive-charts#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "eslint": "^3.7.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-fp": "^2.3.0",
    "flow-bin": "^0.38.0",
    "html-webpack-plugin": "^2.26.0",
    "mocha": "^3.2.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "peerDependencies": {
    "d3": "^4.4.4"
  },
  "dependencies": {
    "ramda": "^0.23.0"
  }
}
